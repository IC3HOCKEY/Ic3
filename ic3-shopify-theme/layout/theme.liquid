<!DOCTYPE html>
<html lang="sv">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="theme-color" content="#0a1c2c">
  <meta name="description" content="{{ page_description | default: shop.description | escape }}">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  
  <!-- Canonical URL -->
  <link rel="canonical" href="{{ canonical_url }}">
  
  <!-- Preload critical assets -->
  <link rel="preload" href="{{ 'theme.css.liquid' | asset_url }}" as="style">
  <link rel="preload" href="{{ 'theme.js' | asset_url }}" as="script">
  
  <title>{% if template == 'index' %}{{ shop.name }}{% else %}{{ page_title }} | {{ shop.name }}{% endif %}</title>
  
  <!-- Favicon -->
  <link rel="icon" href="{{ 'logaic3.png' | asset_img_url: '32x32' }}" type="image/png">
  
  <!-- Preconnect to external domains -->
  <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  
  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&family=Barlow+Condensed:wght@400;700&display=swap" rel="stylesheet">
  
  <!-- Theme CSS -->
  {{ 'theme-settings.css.liquid' | asset_url | stylesheet_tag }}
  {{ 'theme.css.liquid' | asset_url | stylesheet_tag }}
  
  <!-- Shopify required scripts -->
  {{ content_for_header }}
  
  <!-- Theme settings -->
  <script>
    window.theme = {
      strings: {
        addToCart: '{{ 'products.product.add_to_cart' | t }}',
        soldOut: '{{ 'products.product.sold_out' | t }}',
        unavailable: '{{ 'products.product.unavailable' | t }}'
      },
      moneyFormat: {{ shop.money_format | json }}
    };
  </script>
</head>

